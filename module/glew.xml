<?xml version="1.0" encoding="UTF-8"?>
<external-library>
  <name>The OpenGL Extension Wrangler Library</name>
  <bundled-library>
    <libname pattern="glew"/>
    <public>
      <header-path path="include"/>
    </public>
    <src path="src/glew.c"/>
    <group path="include/GL">
      <src path="glew.h"/>
      <src path="glxew.h"/>
      <src path="wglew.h"/>
    </group>
  </bundled-library>
  <pkg-config>glew</pkg-config>
</external-library>
